<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syg.ifmserver.dao.PsnInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.syg.ifmserver.po.PsnInfoPo">
        <id column="id" property="id" />
        <result column="account" property="account" />
        <result column="user_code" property="userCode" />
        <result column="user_name" property="userName" />
        <result column="user_passward" property="userPassward" />
        <result column="email" property="email" />
        <result column="phone" property="phone" />
        <result column="dept_code" property="deptCode" />
        <result column="superior_code" property="superiorCode" />
        <result column="state" property="state" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>
    <select id="searchList" resultType="com.syg.ifmserver.po.PsnInfoPo">
        select * from psn_info limit 100
    </select>


</mapper>
